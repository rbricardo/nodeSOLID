{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off",
        "comma-dangle": ["error", "always-multiline"],
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-useless-constructor": "off"
    }
}
